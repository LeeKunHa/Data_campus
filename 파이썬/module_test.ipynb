    "'''\n",
    "    모듈명 : module_test\n",
    "    mean : 리스트 평균\n",
    "    calc : 두 수의 사칙연산 결과\n",
    "'''\n",
    "# 리스트 평균 구하는 함수\n",
    "def mean(nums):\n",
    "    return sum(nums) / len(nums)\n",
    "\n",
    "# 두 수의 사칙연산 결과를 구하는 함수\n",
    "def calc(x, y):\n",
    "    add = x + y\n",
    "    sub = x - y\n",
    "    mult = x * y\n",
    "    div = x/y\n",
    "    return add, sub, mult, div"
   ]
  }
